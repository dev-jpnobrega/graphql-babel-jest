{
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
  },
  "rules": {
      "indent": ["error", 4],
      "quotes": ["error", "single"],
      "semi": ["error", "never"]
  },
  "env": {
      "node": true,
      "es6": true,
      "jest": true
  }  
}